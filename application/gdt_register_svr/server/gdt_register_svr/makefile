#PATH_PRJ_ROOT是项目的根目录相对于当前目录的相对路径，必须保证正确
PATH_PRJ_ROOT:=$(if $(PATH_PRJ_ROOT), $(PATH_PRJ_ROOT), ../../../..)

#这里的规则是所有的文件一起生成一个可执行文件
#需要依赖的库和头文件请在这里定义
INC= $(INC_KG_KAFKA) $(INC_MUSIC_KAFKA) $(INC_KG_OPENAPI_COMM) $(INC_UUID) $(INC_CALLSRF) $(INC_FUNCLIB) $(INC_UPLOADAPI) $(INC_DTS_CLIENT_NEW) $(INC_OIDB) $(INC_CURL) $(INC_TLS_SIG_API) $(INC_HASH) $(INC_EMOJI_TRANS)  $(INC_BOOST) -I include -I../comm/include -I $(SRC_SPP_SHARE) $(INC_MD5) $(INC_JSONCPP) $(INC_KG_ENCRYPTION) $(INC_STRING_DEAL) $(INC_TBASE) $(INC_QZAPP_FRAME) $(SPP_INC) $(INC_BASECLASS)  $(INC_CURL_WRAP) $(INC_PROTOCOL) $(INC_CMEM_JCE) $(INC_L5) 
LIB= $(LIB_KG_KAFKA)  $(LIB_MUSIC_KAFKA) $(LIB_KG_OPENAPI_COMM) $(LIB_UUID) $(LIB_CALLSRF) $(LIB_FUNCLIB) $(LIB_UPLOADAPI_MT) $(LIB_DTS_CLIENT_NEW) $(LIB_OIDB) $(LIB_CURL_WRAP) $(LIB_CURL) $(LIB_TLS_SIG_API) $(LIB_HASH) $(LIB_DTS_CLIENT_NEW) $(LIB_MOD_LOG) $(LIB_QZAPP_FRAME_MT) $(LIB_KG_ENCRYPTION)  $(LIB_OPENSSL) $(LIB_MD5) $(LIB_TBASE) $(LIB_LOGAPI) $(LIB_CMEM_JCE) -lpthread $(LIB_PROTOCOL) $(LIB_JSONCPP) $(LIB_L5) $(LIB_WEBAPP_FRAME_MT) $(LIB_BASECLASS)

#需要生成的可执行文件，如果是动态库，一定要用.so后缀名
TARGET=gdt_register.so

#如果不是在本目录下，则需要定义以下变量
SRC_DIR=./src/
OBJS_DIR=./src/
BIN_DIR=./bin/

include $(PATH_PRJ_ROOT)/makeinclude/makefile.bincomm
C_FLAGS += -DDEF_USE_API -DDEF_USE_MT
